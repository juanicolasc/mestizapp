<tr id="<%= dom_id item %>">
  <th scope="row"><%= item.product.name %></th>
  <td><%= item.quantity %></td>
  <td><%= item.observations %></td>
  <td><%= item.price_unit %></td>
  <td><%= item.total %></td>
  <td><%= link_to content_tag(:i, " ", :class => "bi bi-zoom-in"), item.product, :class => "btn btn-success", :title => "Ver en Detalle", 'data-bs-toggle' => "tooltip", 'data-bs-placement' => "bottom" %></td>
</tr>
